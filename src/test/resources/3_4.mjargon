ts = time signature 3/4

subbeat    = rhythm [X.x.X.x.X.x.] on ts
beat       = rhythm [X...X...X...] on ts
main       = rhythm [x.xx..xx..x.] on ts
poly       = rhythm [x..x..x..x..] on ts
sixteenths = rhythm [xxxxxxxxxxxx] on ts
whole      = rhythm [x...........] on ts

hihat          = drum sequence hh
ride           = drum sequence rd
bassdrumrhythm = drum sequence bd   bd bd   bd bd    sn
tutupa         = drum sequence bd bd sn

chords = chord progression E3AC4♯ D3GB FAC4 D3FA EG♯B
seq1 = sequence A4 B B A C5♯ E
seq2 = sequence D5 C C B4 B _
seq3 = sequence A4 B B C5 A4 _
seq4 = sequence A4 B C5 F
seq5 = sequence E5
             
